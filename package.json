{
  "name": "tse-id-js",
  "version": "0.0.1",
  "description": "Node.js package to fetch TSE system details from BSI website",
  "main": "dist/index.js",
  "bin": {
    "tse-id-js": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc --build",
    "build:clean": "rm -rf dist && tsc",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "ts-node src/cli.ts",
    "scrape": "npm run build && node dist/cli.js",
    "scrape:fast": "npm run build && node dist/cli.js --headless",
    "perf:test": "node scripts/performance-test.js",
    "docker:build": "./scripts/docker-build.sh ubuntu",
    "docker:build:alpine": "./scripts/docker-build.sh alpine",
    "docker:build:all": "./scripts/docker-build.sh both",
    "docker:test": "docker run --rm tse-scraper:latest --help"
  },
  "keywords": [
    "tse",
    "bsi",
    "germany",
    "fiscal",
    "scraping",
    "playwright"
  ],
  "author": "rechtlogisch",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "playwright": "^1.40.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}


